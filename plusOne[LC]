class Solution(object):
    def plusOne(self, digits):
        num = int(''.join(map(str, digits))) # coverting  arr to int
        num = num + 1
        digit = [int(x) for x in str(num)]#int to arr
        return digit
Method:2
class Solution(object):
    def plusOne(self, digits):
        n = len(digits)
        for i in range (n-1,-1,-1):
            if(digits[i]<9):
                digits[i] =digits[i]+1
                return digits
            else:
                digits[i]=0
        return [1] + digits
